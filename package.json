{
  "name": "NodePract",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "global": "^4.4.0",
    "http-status": "^1.7.4",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.3",
    "superstruct": "^1.0.4",
    "validator": "^13.12.0"
  },
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "npx tsc",
    "start2": "cross-env NODE_ENV=development nodemon ts-node -esm src/index.ts",
    "start": "cross-env NODE_ENV=development tsc-watch --onSuccess \"node ./dist/index.js\"",
    "start3": "tsc --watch & cross-env NODE_ENV=development node --watch dist/index.js",
    "startTs": "cross-env NODE_ENV=development node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "kill": "sudo lsof -i :3010"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.11",
    "@types/validator": "^13.11.10",
    "concurrently": "^8.2.2",
    "eslint": "^9.2.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0"
  }
}
